@model PdfReport.Api.Features.Report.SampleReport
@{
    Layout = "_ReportLayout";
}

<table>
    <thead>
    <tr>
        <th style="text-align: center" >Sample Alpha</th>
        <th style="text-align: center" >Sample Beta</th>
        <th style="text-align: center" >Sample Gama</th>
    </tr>
    </thead>
    <tbody>
    @for (int i = 0; i < Model.Rows.Count; i++)
    {
        <tr>
            <td style="text-align: center"  value-for="Rows[i].SampleFoo"></td>
            <td style="text-align: center" value-for="Rows[i].SampleBar"></td>
            <td style="text-align: center" value-for="Rows[i].SampleBaz"></td>
        </tr>
    }
    </tbody>
</table>