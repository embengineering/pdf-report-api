@model PdfReport.Api.Features.Report.SampleReport
@{
    Layout = "_ReportLayout";
}

<table>
    <thead>
    <tr>
        <th>Sample Alpha</th>
        <th>Sample Beta</th>
        <th>Sample Gama</th>
    </tr>
    </thead>
    <tbody>
    @for (int i = 0; i < Model.Rows.Count; i++)
    {
        <tr style="height: 25px;">
            <td>
                @Html.DisplayFor(x => Model.Rows[i].SampleFoo)
            </td>
            <td class="text-center">
                @Html.DisplayFor(x => Model.Rows[i].SampleBar)
            </td>
            <td class="text-center">
                @Html.DisplayFor(x => Model.Rows[i].SampleBaz)
            </td>
        </tr>
    }
    </tbody>
</table>